{
  "addons": [
    "newrelic",
    "papertrail",
    "heroku-postgresql",
    "sendgrid"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "DATABASE_DATABASE": {
      "required": true
    },
    "DATABASE_HOST": {
      "required": true
    },
    "DATABASE_PASSWORD": {
      "required": true
    },
    "DATABASE_USERNAME": {
      "required": true
    },
    "ENGINE_API_KEY": {
      "required": true
    },
    "NEW_RELIC_LICENSE_KEY": {
      "required": true
    },
    "NEW_RELIC_LOG": {
      "required": true
    },
    "NODE_ENV": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "RSA_PRIVATE_KEY": {
      "required": true
    },
    "RSA_PUBLIC_KEY": {
      "required": true
    },
    "SENDGRID_API_KEY": {
      "required": true
    },
    "SENDGRID_PASSWORD": {
      "required": true
    },
    "SENDGRID_USERNAME": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "rarareserve-api-ts",
  "scripts": {
  },
  "stack": "heroku-18"
}
